{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ],
  "buildCommand": "nest build",  
  "outputDirectory": "dist",
  "installCommand": "npm install --legacy-peer-deps"
}